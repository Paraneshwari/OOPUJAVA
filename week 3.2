import java.util.Scanner;
public class Arr{
    public static void main(String args[]){
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        int[] a=new int[n];
        for(int i=0;i<n;i++){
            a[i]=input.nextInt();
        }
        int[]b=new int[n];
        int max=a[0];
        for(int i=0;i<n;i++){
            if(a[i]>max)
                max=a[i];
        }
        for(int i=0;i<n;i++){
            b[i]=a[i]-max;
        }
        for(int i=0;i<n;i++){
            b[i]=b[i]*max;
        }
        for(int i=0;i<n;i++){
            System.out.print(b[i]+" ");
        }
    }
}
