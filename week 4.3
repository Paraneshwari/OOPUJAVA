class Student{
    private String name;
    private int rollno;
    public Student(){
        this.name=null;
        this.rollno=0;
        System.out.println("No-arg constructor is invoked");
    }
    
    public Student(String name){
        this.name=name;
        this.rollno=0;
        System.out.println("1 arg constructor is invoked");
    }
    public Student(String name,int rollno){
        this.name=name;
        this.rollno=rollno;
        System.out.println("2 arg constructor is invoked");
    }
    public void display(){
        System.out.println("Name ="+name+" , Roll no = "+rollno);
    }
}
public class Test{
    public static void main(String args[]){
        Student s1=new Student();
        Student s2=new Student("Rajalakshmi");
        Student s3=new Student("Lakshmi",101);
        s1.display();
        s2.display();
        s3.display();
    }
}
